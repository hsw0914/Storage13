@charset "utf-8";
/* CSS Document */
/* clear */
*{
	margin: 0;
	padding: 0;
}
a{
	text-decoration: none;
	color: #454545;
}
li{
	margin: 15px;
	list-style: none;
	float: left;
	color: #454545;
}

.wrapper{
	width: 100%;
	height: 1600px;
}
/* header */
.header{
	position: relative;
	width: 1024px;
	height: 150px;
	margin: 0 auto;
}
.tnb{
	position: absolute;
	right: 0;
	top: 10px;
}
/* ani */
.ani{
	width: 100%;
	height: 450px;
}
/* con */
.con{
	width: 1024px;
	height: 870px;
	margin: 0 auto;
}
/* footer */
.footer{
	width: 100%;
	height: 130px;
	background-color: #454545;
}